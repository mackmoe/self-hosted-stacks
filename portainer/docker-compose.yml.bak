name: portainer
services:
  portainer:
    container_name: mgmt-stack-portainer
    hostname: portainer
    image: portainer/portainer-ee:latest
    ports:
      - mode: ingress
        target: 9000
        published: "32772"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: /data/compose/portainer
        target: /data
        bind:
          create_host_path: true
      - type: bind
        source: /var/run/docker.sock
        target: /var/run/docker.sock
        bind:
          create_host_path: true
    devices: []
    cap_add: []
    command: []
    environment: []
    networks:
      - portainer
    privileged: true
    cpu_shares: 90
    deploy:
      resources:
        limits:
          memory: 3790M
networks:
  default:
    name: portainer_default
  portainer:
    name: portainer
x-casaos:
  hostname: portainer.molovestoshare.com
  scheme: http
  index: /
  port_map: "32772"
  author: self
  category: self
  icon: https://linuxiac.b-cdn.net/wp-content/uploads/2023/06/portainer.jpg
  title:
    custom: Portainer Docker UI
